{
  "appId": "com.dario.sauronapp",
  "appName": "Sauron",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#ffffff",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#ffffff"
    },
    "Keyboard": {
      "resize": "body",
      "style": "DARK"
    }
  },
  "server": {
    "url": "http://192.168.1.8:9500",
    "cleartext": true
  }
}
